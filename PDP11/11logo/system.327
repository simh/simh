	MOV #DCTSMS,A	;FIND A SMALL MESSAGE SLOT
	TST (A)+
	BNE .-2
	MOV #<145_10>,B
	BIS DCRMID,B
	ADD #40,B
	SWAB B
	MOV B,-(A)
	SPOP B
	JMP RECLOP

NODATA:	JMP RECLOP

;GET 16 BIT WORD OFF THE LINE
;6 BITS, 6 BITS, 2 PARITY+4 BITS (PPBBBB)
