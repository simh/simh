	BNE GWORDB	;NO, HAVE GOT BAD WORD
	ADD A,DCRCKS	;YES, ADD TO CHECKSUM
	SPOP B
	JMP SRET
GWORDB:	SPOP B
	RTS PC

;GET 6 BITS OFF THE LINE
GBITCH:	JSR PC,RFLUSH
